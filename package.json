{
  "dependencies": {},
  "devDependencies": {},
  "scripts": {
    "copy-fonts": "cpx \"./node_modules/@fortawesome/fontawesome-free/webfonts/**/*\" ./app/assets/builds/webfonts",
    "build-css": "sass ./app/assets/stylesheets/application.scss ./app/assets/builds/application.css --no-source-map",
    "build:css:fontawesome": "yarn copy-fonts && yarn build-css"
  }
}
